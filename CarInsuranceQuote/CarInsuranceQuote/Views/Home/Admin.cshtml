@using CarInsuranceQuote.Models
@model List<Quote>

<div class="row no-gutters m-3">
     <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
          <div class="card-header text-center">
              <h5>Quotes</h5>
          </div>
         <div class="card-body">
            @foreach (var quote in Model)
            {
             <div class="row">
                 <div class="col">
                     <h6>First Name</h6>
                     @quote.FirstName
                 </div>
                 <div class="col">
                     <h6>Last Name</h6>
                     @quote.LastName
                 </div>
                 <div class="col">
                     <h6>Email Address</h6>
                     @quote.EmailAddress
                 </div>
                 <div class="col">
                     <h6>Quote</h6>
                 </div>
             </div>
            }
         </div>
     </div>
</div>

